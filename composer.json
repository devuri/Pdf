{
    "name": "ucsdmath/pdf",
    "description": "A adapter for the mPDF print library",
    "keywords": ["pdf", "mpdf", "print", "ucsdmath"],
    "homepage": "https://github.com/UCSDMath",
    "type": "library",
    "license": "MIT",
    "time": "2015-12-05",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "support": {
        "source": "https://github.com/UCSDMath/Pdf",
        "email": "deisner@ucsd.edu"
    },
    "require": {
        "php": ">=5.6.10",
        "mpdf/mpdf": "6.0.0",
        "smalot/pdfparser": "0.9.25",
        "ucsdmath/functions": "1.5.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "squizlabs/php_codesniffer": "2.4.*"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Pdf\\": "src/Pdf/"
        }
    },
    "suggest": {
        "twig/twig": "Incorporate a flexible, fast, and secure template engine for PHP"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    }
}
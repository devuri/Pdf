{
    "name": "ucsdmath/pdf",
    "type": "library",
    "description": "A adapter for the mPDF print library.",
    "keywords": ["pdf","ucsdmath", "mpdf", "print", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2017-10-29",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.1",
        "mpdf/mpdf": "6.1.3",
        "twig/twig": "2.4.4",
        "nesbot/carbon": "1.22.1",
        "twig/extensions": "1.5.1",
        "ucsdmath/functions": "1.29.x-dev"
    },
    "require-dev": {
        "symfony/debug": "3.3.10",
        "mockery/mockery": "1.0.0",
        "phpunit/phpunit": "6.4.3",
        "fzaninotto/faker": "1.7.1",
        "ucsdmath/date": "1.29.x-dev",
        "symfony/var-dumper": "3.3.10",
        "symfony/browser-kit": "3.3.10",
        "symfony/dom-crawler": "3.3.10",
        "symfony/options-resolver": "3.3.10",
        "squizlabs/php_codesniffer": "3.1.1"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Pdf\\": "src/Pdf/"
        }
    },
    "suggest": {
        "twig/twig": "Incorporate a flexible, fast, and secure template engine for PHP",
        "mikehaertl/pdftk2-precise-amd64": "pdftk 2 binary for Ubuntu 12.04 LTS (amd64)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.29-dev"
        }
    },
    "minimum-stability": "dev"
}

{
    "name": "ucsdmath/pdf",
    "type": "library",
    "description": "A adapter for the mPDF print library",
    "keywords": ["pdf","ucsdmath", "mpdf", "print", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2016-03-01",
    "authors": [
        {"name": "Daryl Eisner", "email": "deisner@ucsd.edu"}
    ],
    "support": {
        "source": "https://github.com/UCSDMath/Pdf",
        "email": "deisner@ucsd.edu"
    },
    "require": {
        "php": "^7.0",
        "mpdf/mpdf": "6.1.1",
        "smalot/pdfparser": "0.9.25",
        "mikehaertl/php-pdftk": "0.2.4",
        "ucsdmath/functions": "1.7.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "0.9.5",
        "phpunit/phpunit": "5.3.4",
        "ucsdmath/testing": "1.7.x-dev",
        "squizlabs/php_codesniffer": "2.6.0"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Pdf\\": "src/Pdf/"
        }
    },
    "suggest": {
        "twig/twig": "Incorporate a flexible, fast, and secure template engine for PHP",
        "mikehaertl/pdftk2-precise-amd64": "pdftk 2 binary for Ubuntu 12.04 LTS (amd64)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7-dev"
        }
    }
}
